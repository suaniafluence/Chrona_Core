# Frontend env example (kiosk)
VITE_API_URL=http://localhost:8000

# Kiosk configuration
VITE_KIOSK_ID=1
VITE_PUNCH_TYPE=clock_in

# Kiosk API key (generate via POST /admin/kiosks/{id}/generate-api-key)
# For development/testing, use: your-api-key-here
VITE_KIOSK_API_KEY=your-api-key-here

# Kiosk mode settings
# Auto-enter kiosk mode on startup (true/false)
VITE_AUTO_KIOSK_MODE=false
