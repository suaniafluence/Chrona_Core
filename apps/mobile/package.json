{
  "name": "chrona-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:e2e:ios": "detox build-ios-framework",
    "build:e2e:android": "detox build-android-framework",
    "build:e2e": "detox build-framework-cache",
    "e2e:ios": "detox test --configuration ios --cleanup",
    "e2e:android": "detox test --configuration android --cleanup",
    "e2e": "detox test --cleanup",
    "e2e:debug": "detox test --configuration ios --debug",
    "e2e:record": "detox test --configuration ios --record-logs all"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.1.8",
    "axios": "^1.12.2",
    "expo": "54.0.21",
    "expo-asset": "~12.0.9",
    "expo-build-properties": "~1.0.9",
    "expo-dev-client": "~6.0.16",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-local-authentication": "~17.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-device-info": "^14.1.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
